{
  "name": "maskify-react",
  "version": "0.0.1",
  "type": "module",
  "description": "ðŸŽ¯ A simple, powerful, and reliable React component for masked input.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite --config vite.demo.config.ts",
    "build": "npx tsc --noEmit && vite build --config vite.lib.config.ts",
    "build:demo": "vite build --config vite.demo.config.ts",
    "build:all": "npm run build && npm run build:demo",
    "preview": "vite preview --config vite.demo.config.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "npx tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "typescript",
    "input",
    "mask",
    "input-mask",
    "formatted-input",
    "autofill",
    "autocomplete",
    "phone-mask",
    "credit-card-mask",
    "date-mask",
    "ssr",
    "server-side-rendering"
  ],
  "author": "Igor Barzakh <igorbarzakh@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jsdom": "^26.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "terser": "^5.43.1",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/igorbarzakh/maskify.git"
  },
  "bugs": {
    "url": "https://github.com/igorbarzakh/maskify/issues"
  },
  "homepage": "https://github.com/igorbarzakh/maskify#readme"
}
